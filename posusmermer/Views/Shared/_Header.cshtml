
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

<head>
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />
    <meta name="author" content="INSPIRO" />
    <meta name="description" content="Themeforest Template Polo">
    <!-- Document title -->
    <title>POLO | The Multi-Purpose HTML5 Template</title>
    <!-- Stylesheets & Fonts -->
    <link href="/posus/css/plugins.css" rel="stylesheet">
    <link href="/posus/css/style.css" rel="stylesheet">
    <link href="/posus/css/responsive.css" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
          integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.8.1/font/bootstrap-icons.css">
    <link rel="stylesheet" href="https://unpkg.com/swiper/swiper-bundle.min.css" />



</head>

<body data-animation-in="fadeIn" data-animation-out="fadeOut" data-icon="2" data-icon-color="#072a16"
      data-speed-in="1000" data-speed-out="1000">

    <!-- Body Inner -->
    <div class="body-inner">

        <!-- Header -->
        <header id="header">
            <div class="header-inner">
                <div class="container">
                    <!--Logo-->
                    <div id="logo">
                        <a href="index.html" class="logo" data-src-dark="/posus/images/logo-dark.png">
                            <img src="/posus/images/logo.png" alt="Polo Logo">
                        </a>
                    </div>
                    <!--End: Logo-->
                    <!-- Search -->
                    <div id="search">
                        <div id="search-logo"><img src="/posus/images/logo.png" alt="Polo Logo"></div>
                        <button id="btn-search-close" class="btn-search-close" aria-label="Close search form">
                            <i class="icon-x"></i>
                        </button>
                        <form class="search-form" action="search-results-page.html" method="get">
                            <input class="form-control" name="q" type="search" placeholder="Search..."
                                   autocomplete="off" autocorrect="off" autocapitalize="off" spellcheck="false" />
                            <span class="text-muted">Start typing & press "Enter" or "ESC" to close</span>
                        </form>

                        <div class="search-suggestion-wrapper">


                            <div class="search-suggestion">
                                <h3>News Articles</h3>
                                <p><a href="#">Beautiful nature, and rare feathers!</a></p>
                                <p><a href="#">New costs and rise of the economy!</a></p>
                                <p><a href="#">A true story, that never been told!</a></p>
                            </div>
                            <div class="search-suggestion">
                                <h3>Looking for these?</h3>
                                <p><a href="#">New costs and rise of the economy!</a></p>
                                <p><a href="#">AI can be trusted to take answer calls </a></p>
                                <p><a href="#">Polo now lets you easily create any beautiful clean website</a></p>
                            </div>
                            <div class="search-suggestion">
                                <h3>Blog Posts</h3>
                                <p><a href="#">A true story, that never been told!</a></p>
                                <p><a href="#">Beautiful nature, and rare feathers!</a></p>
                                <p><a href="#">The most happiest time of the day!</a></p>
                            </div>


                        </div>
                    </div>
                    <!-- end: search -->
                    <!--Header Extras-->
                    <div class="header-extras">
                        <ul>
                            <li>
                                <!--search icon-->
                                <a id="btn-search" href="#"> <i class="icon-search1"></i></a>
                                <!--end: search icon-->
                            </li>

                        </ul>
                    </div>
                    <!--end: Header Extras-->
                    <!--Navigation Resposnive Trigger-->
                    <div id="mainMenu-trigger">
                        <button class="lines-button x"> <span class="lines"></span> </button>
                    </div>
                    <!--end: Navigation Resposnive Trigger-->
                    <!--Navigation-->
                    <div id="mainMenu">
                        <div class="container">
                            <nav>
                                <ul>
                                    <li><a href="index.html">Home</a></li>
                                    <li><a href="index.html">Ürünler</a></li>
                                    <li><a href="index.html">Plakalar</a></li>
                                    <li><a href="index.html">Projeler</a></li>
                                    <li><a href="index.html">Katalog</a></li>
                                    <li><a href="index.html">Hakkında</a></li>
                                    <li><a href="index.html">İletişim</a></li>
                                </ul>
                            </nav>
                        </div>
                    </div>
                    <!--END: NAVIGATION-->
                </div>
            </div>
        </header>
        <!-- Swiper -->
        <div class="swiper mySwiper mt-5">
            <div class="swiper-wrapper">
                <div class="swiper-slide">
                    <h3 style="position: absolute; color: white; bottom: 70px;">Başlık</h3>
                    <h5 style="position: absolute; color: white; bottom: 50px;">Lorem ipsum dolor sit amet consectetur adipisicing elit. Sed, asperiores.</h5>
                    <img src="/posus/src/slide-carrara.jpg" alt="">
                </div>
                <div class="swiper-slide">
                    <h3 style="position: absolute; color: white; bottom: 70px;">Başlık</h3>
                    <h5 style="position: absolute; color: white; bottom: 50px;">Lorem ipsum dolor sit amet consectetur adipisicing elit. Sed, asperiores.</h5>
                    <img src="/posus/src/slide-ivory.jpg" alt="">
                </div>
                <div class="swiper-slide">
                    <h3 style="position: absolute; color: white; bottom: 70px;">Başlık</h3>
                    <h5 style="position: absolute; color: white; bottom: 50px;">Lorem ipsum dolor sit amet consectetur adipisicing elit. Sed, asperiores.</h5>
                    <img src="/posus/src/slide-patlatma.jpg" alt="">
                </div>
                <div class="swiper-slide">
                    <h3 style="position: absolute; color: white; bottom: 70px;">Başlık</h3>
                    <h5 style="position: absolute; color: white; bottom: 50px;">Lorem ipsum dolor sit amet consectetur adipisicing elit. Sed, asperiores.</h5>
                    <img src="/posus/src/slide-tundra-darkj.jpg" alt="">
                </div>
            </div>
            <div class="swiper-button-next"></div>
            <div class="swiper-button-prev"></div>
        </div>

        <!-- Content -->
        <section id="page-content">
            <div class="container">
                <!-- post content -->
                <!-- Page title -->
                <div class="page-title">
                    <h1>Blog - Three Columns</h1>
                    <div class="breadcrumb float-left">
                        <ul>
                            <li>
                                <a href="#">Home</a>
                            </li>
                            <li>
                                <a href="#">Blog</a>
                            </li>
                            <li class="active">
                                <a href="#">Three Columns</a>
                            </li>
                        </ul>
                    </div>
                </div>
                <!-- end: Page title -->
                <!-- Blog -->

                @RenderBody()
                <!-- Pagination -->


                <ul class="pagination">
                    <li class="page-item"><a class="page-link" href="#"><i class="fa fa-angle-left"></i></a></li>
                    <li class="page-item"><a class="page-link" href="#">1</a></li>
                    <li class="page-item"><a class="page-link" href="#">2</a></li>
                    <li class="page-item active"><a class="page-link" href="#">3</a></li>
                    <li class="page-item"><a class="page-link" href="#">4</a></li>
                    <li class="page-item"><a class="page-link" href="#">5</a></li>
                    <li class="page-item"><a class="page-link" href="#"><i class="fa fa-angle-right"></i></a></li>
                </ul>
                <!-- end: Pagination -->

            </div>
            <!-- end: post content -->

        </section>
        <!-- end: Content -->

    </div>
    <!-- end: Body Inner -->
    <!-- Scroll top -->
    <a id="scrollTop"><i class="icon-chevron-up1"></i><i class="icon-chevron-up1"></i></a>

    <!--Abone Ol-->
    <div class="container">
        <h5 style="text-align: center;">Hakkımızda Haberlere Ulaşmak İçin Abone Olun</h5>
        <form class="d-flex bd-highlight mt-4">
            <div class="mb-3 w-50 bd-highlight offset-sm-2">
                <input style="height: 50px;" type="email" class="form-control ps-4" id="exampleInputEmail1"
                       aria-describedby="emailHelp" placeholder="E-mail Adresiniz">
            </div>
            <button style="height: 50px;" type="submit" class="ms-3 btn btn-primary">Abone Ol</button>
        </form>
    </div>

    <!--Footer-->
    <div style="position:relative; bottom: 0px; width: 100%;" class="footer bg-dark p-4 mt-5">
        <div class="container">
            <div class="row">
                <div class="col-sm">
                    <ul class="list-unstyled">
                        <h5 style="color: white;" class="">Ürünlerimiz</h5>
                        <li class="p-1 border-bottom border-secondary"><a href="#">Fayans</a></li>
                        <li class="p-1 border-bottom border-secondary"><a href="#">Mozaik</a></li>
                        <li class="p-1 border-bottom border-secondary"><a href="#">Profil</a></li>
                        <li class="p-1 border-bottom border-secondary">
                            <a href="#">
                                Lavabo & Kurna &
                                Duş Teknesi
                            </a>
                        </li>
                        <li class="p-1 border-bottom border-secondary">
                            <a href="#">
                                Havuz
                                Döşemeleri
                            </a>
                        </li>
                        <li class="p-1 border-bottom border-secondary"><a href="#">Su Jeti</a></li>
                        <li class="p-1"><a href="#">Plaka</a></li>
                    </ul>
                </div>
                <div class="col-sm">
                    <h5 style="color: white;">İletişim</h5>
                    <p style="color: white;">
                        Lorem ipsum, dolor sit amet consectetur adipisicing elit. Commodi, <br>
                        um tempore officiis harum neque perferendis doloremque beatae nihil <br>
                        odio, qui amet sint illo repellendus, molestiae modi quasi! Totam, dolorum amet?
                    </p>
                </div>
            </div>
            <div class="mt-3 text-secondary d-flex justify-content-between">
                <h6 style="color: gray;">© 2022 Umut Yumaklı. All Rights Reserved.</h6>
                <ul class="footer-social d-flex list-unstyled">
                    <li><a href="#"><i class="bi bi-instagram me-2"></i></a></li>
                    <li><a href="#"><i class="bi bi-envelope"></i></a></li>
                </ul>
            </div>
        </div>

    </div>

    <!--Plugins-->
    <script src="/posus/js/jquery.js"></script>
    <script src="/posus/js/plugins.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
            integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
            crossorigin="anonymous"></script>
    <script src="https://unpkg.com/swiper/swiper-bundle.min.js"></script>

    <!--Template functions-->
    <script src="/posus/js/functions.js"></script>
    <script src="https://unpkg.com/swiper/swiper-bundle.min.js"></script>

    <script>
        var swiper = new Swiper(".mySwiper", {
            navigation: {
                nextEl: ".swiper-button-next",
                prevEl: ".swiper-button-prev",
            },
        });
    </script>

</body>

</html>

