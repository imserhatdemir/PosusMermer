@model IEnumerable<EntityLayer.Concrete.About>

<head>
    @foreach (var item in Model)
    {
        <meta name="viewport" content="width=device-width, initial-scale=1" />
        <meta http-equiv="content-type" content="text/html; charset=utf-8" />
        <meta name="author" content="" />
        <meta name="description" content="@item.Description">
        <meta name="keywords" content="@item.Keywords">
        <!-- Document title -->
        <title>@item.Title</title>
}
    <!-- Stylesheets & Fonts -->
    <link runat="server" rel="shortcut icon" href="/Images/favicon.ico" type="image/x-icon" />
    <link runat="server" rel="icon" href="/Images/favicon.ico" type="image/ico" />
    <link href="/posus/css/plugins.css" rel="stylesheet">
    <link href="/posus/css/style.css" rel="stylesheet">
    <link href="/posus/css/responsive.css" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
          integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.8.1/font/bootstrap-icons.css">
    <link rel="stylesheet" href="https://unpkg.com/swiper/swiper-bundle.min.css" />


    <link href="/posus/assets/css/style.css" rel="stylesheet">


</head>
